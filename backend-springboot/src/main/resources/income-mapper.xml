<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wfl.mbtest.ledger.IncomeMapper">
	
	<select id="getIncome" parameterType="Integer" resultType="income">
        SELECT *, (TO_CHAR(INCOMETIME,'YYYY-MM-DD HH:MM:SS')) as incomedTime FROM INCOME WHERE ledgerId = #{ledgerId}
	</select>
	
</mapper>